{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules"
  ],
  "verbose": false,
  "execMap": {
    "js": "node"
  },
  "events": {
    "restart": "echo $(date '+%H:%M:%S') $FILENAME changed."
  },
  "watch": [
    "dist"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}